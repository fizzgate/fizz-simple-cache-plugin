<?xml version="1.0" encoding="UTF-8"?>

<Configuration status="warn">
    <properties>
    </properties>
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d  %-5level [%t]  %c{1}.%M:%L  %m  %ex%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="error">
            <AppenderRef ref="Console"/>
        </Root>
        <Logger name="com.fizzgate" level="error"/>
    </Loggers>
</Configuration>
